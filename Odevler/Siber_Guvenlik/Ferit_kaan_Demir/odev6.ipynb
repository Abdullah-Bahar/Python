def tuple_kesisimi(tuple1, tuple2):
    # İki tuple'ın kesişimini bul
    kesisim = tuple(set(tuple1) & set(tuple2))
    return kesisim

# Örnek kullanım:
tuple1 = (1, 2, 3, 4, 5)
tuple2 = (4, 5, 6, 7, 8)

kesisim = tuple_kesisimi(tuple1, tuple2)
print("Tuple Kesişimi:", kesisim)


