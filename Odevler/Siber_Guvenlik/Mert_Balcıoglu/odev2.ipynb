boy=float(input("Boyunu metre olarak gir "))
kilo=float(input("Kilonu kilogram olarak gir "))
indeks=kilo/(boy*boy)
if indeks<20:
  print("Zayif")
elif 20<= indeks <25:
  print("Normal")
  print("İndeks: ",indeks)
elif 25<=indeks<30:
  print("Asiri kilolu")
  print("İndeks: ",indeks)
else:
  print("Kilo vermeli")
  print("İndeks: ",indeks)
