kelime = input("Bir kelime gir:")
if len(kelime) < 1 :
  print("En az iki harf olmali:(")
else:
  ilkharf=kelime[0]
  sonharf=kelime[-1]
  if ilkharf==sonharf:
    print(f"{kelime} bas ve son harf ayni:)")
  else:
    print(f"{kelime} bas ve son harf farkli:(")
