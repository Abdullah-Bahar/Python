import random
hedef_sayi=random.randint(1,9)

while True:
  tahmin = int(input("1 ile 9 arasında bir sayı yaz: "))

  if tahmin == hedef_sayi:
    print("Dogru",hedef_sayi)
    break
  else:
    print("Yanlis")
1 ile 9 arasında bir sayı yaz: 7
